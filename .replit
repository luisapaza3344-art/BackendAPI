modules = ["rust", "go", "python-3.11", "postgresql-16", "nodejs-20"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0", "javascript_stripe:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Payment Gateway"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Security Service"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Auth Service"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Advanced Analytics"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ultra Inventory System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ultra Shipping Service"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ultra Professional Frontend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Enterprise API Gateway"

[[workflows.workflow]]
name = "Payment Gateway"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/rust/payment-gateway && CARGO_BUILD_JOBS=2 cargo run --no-default-features --features \"ml-minimal pq-dilithium pq-kyber\" --bin payment-gateway"

[[workflows.workflow]]
name = "Security Service"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/rust/security-service && cargo run"
waitForPort = 8000

[[workflows.workflow]]
name = "Auth Service"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/go/auth-service && SERVER_PORT=8099 TLS_ENABLED=false go run cmd/main.go"
waitForPort = 8099

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Advanced Analytics"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/python/advanced-analytics-service && python main.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Ultra Inventory System"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/rust/ultra-inventory-system && RUST_LOG=info INVENTORY_PORT=3000 DATABASE_URL=$DATABASE_URL REDIS_URL=\"redis://localhost:6379\" cargo run --release"
waitForPort = 3000

[[workflows.workflow]]
name = "Ultra Shipping Service"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/rust/ultra-shipping-service && PORT=6800 cargo run --release"
waitForPort = 6800

[[workflows.workflow]]
name = "Ultra Professional Frontend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd store && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Enterprise API Gateway"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/go/api-gateway && SERVER_PORT=9000 TLS_ENABLED=false go run cmd/main.go"
waitForPort = 9000

[workflows.workflow.metadata]
outputType = "console"

